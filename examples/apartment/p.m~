
a=[]; %a数据y值 
b=[]; %b数据y值 
c=[18.1039,18.0971,18.1024,18.0985,18.1005,18.106,18.105,18.1075,18.1061,18.1153,18.1065,18.1101,18.1167,18.2231,18.2137,18.2091,18.2182,18.206,18.2166,18.2231,18.2191,18.2191];
plot(a,b,'-*b',a,c,'-or'); %线性，颜色，标记 

axis([0,110,0,25]) %确定x轴与y轴框图大小 
set(gca,'XTick',[0:10:100]) %x轴范围1-6，间隔1 
set(gca,'YTick',[0:3:25]) %y轴范围0-700，间隔100 
legend('Locally','Globally'); %右上角标注 
xlabel('persent(%)') %x轴坐标描述 
ylabel('time（s）') %y轴坐标描述

